<?xml version="1.0"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/6.0.1/rti_dds_qos_profiles.xsd">
    <qos_library name="demo_types_Library">
        <qos_profile name="demo_types_Profile" base_name="BuiltinQosLib::Generic.StrictReliable" is_default_qos="true">
            <datawriter_qos>
                <publication_name>
                    <name>demo_typesDataWriter</name>
                </publication_name>
	            <property>
	                <value>
	                    <element>
	                        <name>dds.data_writer.history.memory_manager.fast_pool.pool_buffer_max_size</name>
	                        <value>4096</value>
	                    </element>
	                </value>
	            </property>
            </datawriter_qos>
            <datareader_qos>
                <subscription_name>
                    <name>demo_typesDataReader</name>
                </subscription_name>
            </datareader_qos>
            <participant_qos>
                <participant_name>
                    <name>demo_typesParticipant</name>
                    <role_name>demo_typesParticipantRole</role_name>
                </participant_name>
            </participant_qos>
        </qos_profile>
    </qos_library>
</dds>
